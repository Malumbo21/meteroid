{
  "extends": "@md/tsconfig/base.json",
  "include": ["src", "types.d.ts"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["esnext"],
    "target": "esnext",
    "module": "CommonJS",
    "esModuleInterop": true,
    "strict": true,
    "allowJs": false,
    "moduleResolution": "node",
    "preserveSymlinks": false,
    "outDir": "dist",
    "baseUrl": ".",
    "types": ["node"]
  }
}
